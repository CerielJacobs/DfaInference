#! /bin/sh
 
# testSample <size> <version>
# Runs samples through the LearnedDFAs and the GeneratingDFAs, and prints
# the correctness ratio.

# Check setting of DFA_HOME
if [ -z "$DFA_HOME" ];  then
    DFA_HOME=`pwd`
fi
export DFA_HOME

$DFA_HOME/bin/satin-run -Xmx256M test.TestSamples -s $1 -v $2
